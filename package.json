{
    "name": "online-banking",
    "version": "1.0.0",
    "description": "Main website for Online Banking",
    "scripts": {
        "init:git-hooks": "./scripts/init-git-hooks.sh",
        "format:check": "prettier -c $(git diff --name-only --staged --diff-filter=AM | grep -E '.((t|j)sx?|json|scss)$' | xargs)",
        "format:write": "prettier -w $(git diff --name-only --staged --diff-filter=AM | grep -E '.((t|j)sx?|json|scss)$' | xargs)",
        "lint": "eslint --no-warn-ignored $(git diff --name-only --staged --diff-filter=AM | grep -E '.((t|j)sx?)$' | xargs)",
        "test": "jest",
        "build": "NODE_ENV=production tsx ./build/cmd/build.ts",
        "dev": "NODE_ENV=development tsx watch ./build/cmd/dev-server.ts",
        "bundle-analyzer": "BUNDLE_ANALYZER=1 NODE_ENV=production tsx ./build/cmd/build.ts",
        "dev-server": "tsx watch ./dev-server/server.ts"
    },
    "devDependencies": {
        "@eslint/js": "^9.35.0",
        "@fastify/compress": "^8.1.0",
        "@fastify/static": "^8.2.0",
        "@swc/core": "^1.13.5",
        "@swc/jest": "^0.2.39",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.4.0",
        "@types/react": "^19.1.13",
        "@types/react-dom": "^19.1.9",
        "browserslist": "^4.26.2",
        "css-loader": "^7.1.2",
        "eslint": "^9.35.0",
        "eslint-plugin-jest": "^29.0.1",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "fastify": "^5.6.0",
        "generic-names": "^4.0.0",
        "html-webpack-plugin": "^5.6.4",
        "jest": "^30.1.3",
        "jest-environment-jsdom": "^30.1.2",
        "mini-css-extract-plugin": "^2.9.4",
        "postcss": "^8.5.6",
        "postcss-loader": "^8.2.0",
        "postcss-preset-env": "^10.3.1",
        "prettier": "^3.6.2",
        "sass": "^1.92.1",
        "sass-loader": "^16.0.5",
        "style-loader": "^4.0.0",
        "swc-loader": "^0.2.6",
        "thread-loader": "^4.0.4",
        "tsconfig-paths-webpack-plugin": "^4.2.0",
        "tsx": "^4.20.5",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.43.0",
        "webpack": "^5.101.3",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-dev-server": "^5.2.2"
    },
    "dependencies": {
        "@hookform/resolvers": "^5.2.2",
        "classnames": "^2.5.1",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-hook-form": "^7.63.0",
        "react-router": "^7.9.1",
        "yup": "^1.7.1"
    },
    "engine": {
        "node": ">=22.14.0"
    }
}
